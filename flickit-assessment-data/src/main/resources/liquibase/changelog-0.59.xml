<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.17.xsd">

    <changeSet id="0.59-01" author="Maziyar Gerami">
        <addColumn tableName="fac_assessment">
            <column name="mode" type="tinyint" defaultValue="1">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="0.59-02" author="Maziyar Gerami">
        <dropUniqueConstraint tableName="fau_space"
                              constraintName="uq_fau_space_code_createdby_deletion_time"/>
    </changeSet>

</databaseChangeLog>
